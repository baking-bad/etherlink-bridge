          (address %ticketer)
          (option %context (pair (bytes %routing_info) (address %rollup)))
          (big_map %metadata string bytes)) ;
  code { PUSH string "INVALID_FA12" ;
         PUSH string "INVALID_FA2" ;
         PUSH string "ROUTER_ENTRYPOINT_NOT_FOUND" ;
