                                 (pair %remove_operator (address %owner) (address %operator) (nat %token_id)))) ;
                     SWAP ;
                     DIG 4 ;
                     SELF_ADDRESS ;
                     PAIR 3 ;
                     NIL (or (pair address address nat) (pair address address nat)) ;
                     SWAP ;
                     LEFT (pair address address nat) ;
                     CONS ;
                     SWAP ;
                     IF_NONE
                       { DROP ; DIG 5 ; FAILWITH }
                       { DIG 7 ; DROP ; PUSH mutez 0 ; DIG 2 ; TRANSFER_TOKENS } } ;
                 DIG 5 ;
                 DIG 4 ;
                 DIG 5 ;
                 PAIR ;
                 SOME ;
                 UPDATE 5 ;
